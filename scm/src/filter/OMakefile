.PHONY: clean

OCAMLPACKS[] = extlib

UseCamlp4(pa_oo pa_openin)

FILES[] =
	binding
	main

PROGRAM       = ../filter
OCAMLOPT      = ocamlopt -for-pack $(capitalize $(basename $(PROGRAM)))
OCAMLOPTLINK  = ocamlopt

MyOCamlPackage($(PROGRAM), $(FILES))
OCamlLibrary(filter, $(FILES))

.DEFAULT:

clean:
	ocaml-clean



