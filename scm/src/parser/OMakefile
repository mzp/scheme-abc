# package
OCAMLPACKS[] =
     extlib

# build
FILES[] =
	token
	parsec
	lexer
	sexp
	lisp
	main

CPPFLAGS+=-I../type

PROGRAM = ../parser
OCAMLOPT      = ocamlopt -for-pack $(capitalize $(basename $(PROGRAM)))
OCAMLOPTLINK  = ocamlopt
MyOCamlPackage($(PROGRAM), $(FILES))
OCamlLibrary(parser, $(FILES))

# phony
.PHONY: clean
.DEFAULT:
clean:
	ocaml-clean closTrans.mli
