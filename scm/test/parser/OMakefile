.PHONY: clean check


################################################
# Build an OCaml program
#

OCAMLINCLUDES += $(SRC)/parser
OUNIT_LIBS += astUtil
OUNIT_SRC_DIR=$(SRC)/parser/

OUnitTest(lexer ,lexer parsec)
OUnitTest(sexp  ,sexp  parsec)
OUnitTest(lisp  ,lisp sexp parsec lexer)


#############################################################
#
# PHONY Target
#

clean:
	ocaml-clean
