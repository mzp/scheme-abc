# package
OCAMLPACKS[] =
	xml-light
	extlib
	oUnit

# build
OCAMLINCLUDES += ../src

OUnitTest(byte   ,byte parsec)
OUnitTest(abc    ,abc parsec disasm byte)
OUnitTest(swfmill,swfmill easyXml abc code byte disasm parsec)
OUnitTest(code   ,code easyXml)

# PHONY TARGET
.PHONY: check clean
clean:
	ocaml-clean
